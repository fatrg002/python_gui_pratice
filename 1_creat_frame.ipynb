    "from tkinter import *\n",
    "root = Tk()\n",
    "root.title(\"Nado GUI\")#視窗TITLE\n",
    "root.geometry(\"640x480+100+300\")#視窗大小(長x寬,+x座標+y座標(視窗顯示位置))\n",
    "root.resizable(False,False)#視窗大小改變(x,y),False代表固定不改變\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "root.mainloop()"
   ]
